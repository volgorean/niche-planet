<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>

<div class="picPanel">
</div>

<div class="panel">
  <div id="map" style='width: 100%;'>

      <%= content_tag "div", id: "multi_overlays", style: 'width: 100%; height: 400px;', data: {coords: [
        [{lat: 40.363288, lng: -3.603516},{lat: 41.658549, lng: -0.909119},{lat: 41.393294, lng: 2.164307}],
        [{lat: 45, lng: 9},{lat: 47, lng: 8},{lat: 44, lng: 6}],
        [{lat: 50, lng: 2}]
        ]} do %>
      <% end %>

  </div>
</div>

<div>
  <h1>Niches</h1>
  <% @niches.each do |niche| %>
    <a href="/niches/<%= niche.id %>">
    <div class="nichePanel button" style="background-image: url(<%= niche.image.url(:medium) %>) ;">
      <h5 class="nicheIconName"><%= niche.name %></h5>
    </div>
  </a>
  <% end %>
</div>
